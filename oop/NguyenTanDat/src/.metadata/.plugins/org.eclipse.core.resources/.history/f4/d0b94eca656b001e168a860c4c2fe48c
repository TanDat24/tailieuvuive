package bai13;

public class CongNhan {	
	private String mHo;
	private String mTen;
	private String mSoSp;
	public CongNhan(String mHo, String mTen, String mSoSp) {
		super();
		this.mHo = mHo;
		this.mTen = mTen;
		this.mSoSp = mSoSp;
	}
	public String getmHo() {
		return mHo;
	}
	public void setmHo(String mHo) {
		this.mHo = mHo;
	}
	public String getmTen() {
		return mTen;
	}
	public void setmTen(String mTen) {
		this.mTen = mTen;
	}
	public String getmSoSp() {
		return mSoSp;
	}
	public void setmSoSp(String mSoSp) {
		if(mSoSp>0)
		this.mSoSp = mSoSp;
	}
	@Override
	public String toString() {
		return "CongNhan [mHo=" + mHo + ", mTen=" + mTen + ", mSoSp=" + mSoSp + "]";
	}
	
	
}
